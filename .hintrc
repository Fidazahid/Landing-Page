    "development"
  ],
  "hints": {
    "no-inline-styles": "off",
    "detect-css-reflows/composite": "off",
    "detect-css-reflows/paint": "off",
    "detect-css-reflows/layout": "off",
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off",
        "button-name": "off",
        "input-button-name": "off"
      }
    ],
    "axe/forms": [
      "default",
      {
        "label": "off"
      }
    ],
    "highest-available-document-mode": "off",
    "button-type": "off",
    "axe/structure": [
      "default",
      {
        "list": "off"
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_ff <= 110",
    "not ios_saf <= 16.4",
    "not safari <= 16.4",
    "not and_chr <= 111",
    "not chrome <= 111",
    "not edge <= 111",
    "not firefox <= 111",
    "not samsung <= 20"
  ]
